<?xml version="1.0" encoding="utf-8"?>
<!-- xml -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- 背景（透明） -->
    <path
        android:fillColor="@android:color/transparent"
        android:pathData="M0,0h100v100h-100z" />

    <!-- 主圆环（深色底） -->
    <path
        android:pathData="M50,10a40,40 0 1,1 -0.001,0"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#4B2E2A"
        android:strokeWidth="16"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <!-- 内侧高光（较亮的棕色，模拟绕环的亮边） -->
    <path
        android:pathData="M50,14a36,36 0 1,1 -0.001,0"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#7A4E3E"
        android:strokeWidth="6"
        android:strokeAlpha="0.7"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <!-- 胡桃木细纹（若干细曲线，半透明） -->
    <path
        android:pathData="M30,34c6,-4 18,-6 30,0"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#6A3F34"
        android:strokeWidth="1.8"
        android:strokeAlpha="0.5"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <path
        android:pathData="M28,46c8,-3 22,-5 37,4"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#6A3F34"
        android:strokeWidth="1.4"
        android:strokeAlpha="0.45"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <path
        android:pathData="M36,62c10,-6 24,-8 34,-2"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#5A352E"
        android:strokeWidth="1.2"
        android:strokeAlpha="0.4"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <!-- 直径线：从右上到左下 -->
    <path
        android:pathData="M78,22L22,78"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#3E241E"
        android:strokeWidth="6"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

    <!-- 直径线中间的轻微高光（使直径有层次） -->
    <path
        android:pathData="M72,28L28,72"
        android:fillColor="@android:color/transparent"
        android:strokeColor="#8A5A4A"
        android:strokeWidth="2"
        android:strokeAlpha="0.25"
        android:strokeLineCap="round"
        android:strokeLineJoin="round" />

</vector>
